package com.project.springmvc.service;

import java.util.List;

import com.project.springmvc.model.Admin;

public interface AdminService {


    Admin findById(Integer id) throws Exception;
     
    void saveAdmin(Admin admin)throws Exception;
     
    void updateAdmin(Admin admin)throws Exception;
     
    void deleteAdminById(Integer id)throws Exception;
 
    List<Admin> findAllAdmins()throws Exception; 
     
    Admin findAdminById(Integer id)throws Exception;
 
 }
